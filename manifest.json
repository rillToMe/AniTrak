{
  "manifest_version": 3,
  "name": "AniTrack",
  "version": "1.0.0",
  "description": "Track progres nonton per series/episode secara manual. Simple, no-guessing.",
  "action": {
    "default_popup": "popup.html",
    "default_title": "AniTrack",
    "default_icon": {
      "16": "icons/icon16.png",
      "24": "icons/icon24.png",
      "32": "icons/icon32.png"
    }
  },
  "permissions": [
    "storage",
    "contextMenus"
],
"options_ui": { 
    "page": "options.html", "open_in_tab": true 
},
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
